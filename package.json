{
  "name": "@observertc/observer-js",
  "version": "1.0.0-beta.2",
  "description": "Server Side NodeJS Library for processing ObserveRTC Samples",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "npm run build",
    "prepublish": "pkgfiles",
    "prepublishOnly": "npm run lint",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.js src/*",
    "build": "tsc",
    "test": "jest --config jest.config.js"
  },
  "keywords": [
    "webrtc",
    "observertc",
    "server",
    "integration"
  ],
  "author": "Balazs Kreith",
  "license": "Apache-2.0",
  "dependencies": {
    "@bufbuild/protobuf": "1.1.1",
    "events": "^3.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@tsconfig/node20": "^1.0.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.9.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "bufferutil": "^4.0.6",
    "eslint": "^8.24.0",
    "jest": "^27.5.0",
    "pkgfiles": "^2.3.0",
    "prettier": "2.8.7",
    "ts-jest": "^27.1.5",
    "typescript": "^4.8.3",
    "ts-node-dev": "^2.0.0",
    "utf-8-validate": "^5.0.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/observertc/observer-js"
  }
}
